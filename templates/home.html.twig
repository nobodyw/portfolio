{% include 'base.html.twig' %}

{% block body %}
<main id="swup">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div data-controller="counter">
                    <button
                            type="button"
                            class="btn btn-outline-info"
                            data-counter-target="count"
                            data-action="click->counter#increment"
                    >
                        Click me !
                    </button>
                    <a href={{ path('redirectTest') }}>Test swup</a>
                </div>
            </div>
        </div>
    </div>
</main>
{% endblock %}

